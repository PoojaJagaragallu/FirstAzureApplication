@model IEnumerable<FirstAzureMVCApp.Models.adventure_users>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Adminstrators</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.id</td>
                <td>@user.firstname</td>
                <td>@user.lastname</td>
                <td>@user.email</td>
            </tr>
        }
    </table>
</body>
</html>
